{
  "name": "board-room",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "pnpm run build && wrangler dev",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "deploy": "pnpm run build && wrangler deploy",
    "cf-typegen": "wrangler types && move worker-configuration.d.ts src/",
    "db:push": "drizzle-kit generate && drizzle-kit migrate --config=drizzle-dev.config.ts",
    "db:push:prod": "drizzle-kit generate && drizzle-kit migrate",
    "db:migrate:prod": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate --config=drizzle-dev.config.ts",
    "db:studio:prod": "drizzle-kit studio",
    "db:studio": "drizzle-kit studio --config=drizzle-dev.config.ts"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-cloudflare": "^4.7.4",
    "@sveltejs/kit": "^2.5.27",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/eslint": "^9.6.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.45.1",
    "globals": "^15.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.4",
    "wrangler": "^3.83.0",
    "sveltekit-search-params": "^2.1.2",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.4.1",
    "@types/better-sqlite3": "^7.6.10",
    "drizzle-kit": "^0.25.0",
    "autoprefixer": "^10.4.19"
  },
  "dependencies": {
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@sveltejs/adapter-cloudflare": "^4.7.3",
    "arctic": "2.0.0-next.9",
    "better-sqlite3": "^10.0.0",
    "bits-ui": "^0.21.12",
    "chess.js": "^1.0.0-beta.6",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.34.1",
    "embla-carousel-svelte": "^8.1.6",
    "formsnap": "^1.0.1",
    "lucide-svelte": "^0.399.0",
    "pusher": "^5.2.0",
    "pusher-js": "8.4.0-rc2",
    "svelte-chess": "^0.11.1",
    "svelte-chessground": "^2.0.2",
    "sveltekit-superforms": "^2.16.0",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
    "vaul-svelte": "^0.3.2",
    "zod": "^3.23.8"
  },
  "type": "module"
}