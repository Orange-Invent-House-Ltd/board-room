{
  "name": "board-room",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "pnpm run build && wrangler dev",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "deploy": "pnpm run build && wrangler deploy",
    "cf-typegen": "wrangler types && move worker-configuration.d.ts src/",
    "db:push": "drizzle-kit push --config=drizzle-dev.config.ts",
    "db:push:prod": "drizzle-kit push",
    "db:migrate:prod": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate --config=drizzle-dev.config.ts",
    "db:studio:prod": "drizzle-kit studio",
    "db:studio": "drizzle-kit studio --config=drizzle-dev.config.ts"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-cloudflare": "^4.7.4",
    "@sveltejs/kit": "^2.7.3",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/better-sqlite3": "^7.6.11",
    "@types/eslint": "^9.6.1",
    "autoprefixer": "^10.4.20",
    "bits-ui": "1.0.0-next.33",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.22.8",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.0",
    "globals": "^15.11.0",
    "lucide-svelte": "^0.399.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "svelte": "^5.1.3",
    "svelte-check": "^4.0.5",
    "sveltekit-search-params": "^2.1.2",
    "tailwind-merge": "^2.5.4",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "wrangler": "^3.83.0"
  },
  "dependencies": {
    "@canutin/svelte-currency-input": "^0.12.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@sveltejs/adapter-cloudflare": "^4.7.4",
    "@types/nprogress": "^0.2.3",
    "arctic": "2.0.0-next.9",
    "better-sqlite3": "^10.1.0",
    "chess.js": "1.0.0-beta.8",
    "drizzle-orm": "^0.33.0",
    "embla-carousel-svelte": "^8.3.0",
    "formsnap": "^1.0.1",
    "nanoid": "^5.0.8",
    "nprogress": "^0.2.0",
    "pusher": "^5.2.0",
    "pusher-js": "8.4.0-rc2",
    "svelte-chess": "^0.11.1",
    "svelte-chessground": "^2.0.3",
    "sveltekit-superforms": "^2.20.0",
    "vaul-svelte": "^0.3.2",
    "zod": "^3.23.8"
  },
  "type": "module"
}